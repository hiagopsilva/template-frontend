{
  "name": "template-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^18.0.17",
    "axios": "^0.19.2",
    "babel-plugin-root-import": "^6.6.0",
    "customize-cra": "^1.0.0",
    "env-cmd": "^10.1.0",
    "eslint-import-resolver-babel-plugin-root-import": "^1.1.1",
    "history": "^5.3.0",
    "lint-staged": "^13.0.3",
    "lodash": "^4.17.21",
    "mobx": "^6.6.1",
    "mobx-persist": "^0.4.1",
    "mobx-react": "^7.5.2",
    "mobx-react-router": "^4.1.0",
    "react": "^18.2.0",
    "react-app-rewired": "^2.2.1",
    "react-dom": "^18.2.0",
    "react-loading": "^2.0.3",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^5.0.1",
    "react-toastify": "^8.0.3",
    "styled-components": "^5.1.1",
    "typescript": "^4.7.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js, jsx}": [
      "eslint --fix"
    ]
  },
  "scripts": {
    "dev": "env-cmd -f .env.dev yarn react-app-rewired start",
    "hml": "env-cmd -f .env.hml yarn react-app-rewired start",
    "prd": "env-cmd -f .env.prd yarn react-app-rewired start",
    "build:dev": "env-cmd -f .env.dev yarn react-app-rewired build",
    "build:hml": "env-cmd -f .env.hml yarn react-app-rewired build",
    "build:prd": "env-cmd -f .env.prd yarn react-app-rewired build",
    "build": "env-cmd -f .env.prd yarn react-app-rewired build",
    "start": "env-cmd -f .env.prd yarn react-scripts start",
    "test": "react-app-rewired test",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "pretty": "prettier --config ./.prettierrc --write \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "lint": "tsc && eslint \"./src/**/*.{js,jsx,ts,tsx}\" --fix"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.2",
    "@types/lodash": "^4.14.183",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.2",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "3.2.0",
    "eslint": "^8.22.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.4.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^4.2.5",
    "prettier": "^2.7.1"
  },
  "resolutions": {
    "@types/react": "~18.0.17"
  }
}
